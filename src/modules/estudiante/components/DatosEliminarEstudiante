<template>

    <label for="">ID</label>
  
    <input v-model="id" type="text" />
    <label for="">Cedula</label>
  
    <input v-model="cedula" type="text" />
  
    <label for="">Nombre</label>
  
    <input v-model="nombre" type="text" />
  
    <label for="">Apellido</label>
  
    <input v-model="apellido" type="text" />
  
    <button @click="guardarEstudiante">Eliminar</button>
  </template>
  
   
  
  <script>
  import { eliminarEstudiante } from "../helpers/EstudianteCliente.js";
  
  export default {
    data() {
      return {
        cedula: null,
  
        nombre: null,
  
        apellido: null,
      };
    },
  
    methods: {
      async eliminarEstudiante() {
        const data = {
          cedula: this.cedula,
  
          nombre: this.nombre,
  
          apellido: this.apellido,
  
          provincia: "pichincha",
        };
  
        await ingresarEstudianteFachada(data);
      },
    },
  };
  </script>
  
   
  
